<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.legoshi.taskmodeller.gui.start.StartController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Select your starting model" />
    <ComboBox fx:id="startCB" prefWidth="150.0" value="Freies Modellieren">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Freies Modellieren" />
                <String fx:value="CTT Modell" />
            </FXCollections>
        </items>
    </ComboBox>
    <Button onAction="#onStartButtonClick" text="Start" />
</VBox>
